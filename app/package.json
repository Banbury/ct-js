{
  "main": "index.html",
  "name": "ctjs",
  "description": "ctjs",
  "version": "0.0.1",
  "homepage": "http://ctjs.ru",
  "window": {
    "icon": "ct_ide.png",
    "toolbar": true,
    "frame": true,
    "width": 1024,
    "height": 720,
    "position": "center",
    "min_width": 380,
    "min_height": 380,
    "resizable": true,
    "fullscreen": false,
    "title": "ctjs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ct-js/ct.git"
  },
  "maintainers": [
    {
      "name": "Cosmo Myzrail Gorynych",
      "email": "cosmomyzrailgorynych@gmail.com",
      "web": "https://github.com/cosmomyzrailgorynych"
    },
    {
      "name": "jiumx60rus",
      "web": "https://github.com/jiumx60rus"
    }
  ],
  "webkit": {
    "plugin": false
  },
  "dependencies": {
    "csswring": "3.0.7",
    "ffmpeg-node": "git://github.com/xonecas/ffmpeg-node.git",
    "fs-extra": "5.0.0",
    "html-minifier": "0.7.2",
    "markdown-it": "3.1.0",
    "maxrects-packer": "^1.0.2",
    "uglify-js": "2.8.29"
  },
  "build": {
    "appId": "rocks.ctjs",
    "nwVersion": "0.26.0",
    "output": "./../builds/",
    "packed": false,
    "strippedProperties": [
      "scripts",
      "devDependencies",
      "build",
      "stylintrc"
    ],
    "targets": [
      "7z"
    ],
    "win": {
      "icon": "./ct.ide.ico",
      "companyName": "Cosmo Myzrail Gorynych",
      "copyright": "© Cosmo Myzrail Gorynych 2016-2017"
    },
    "mac": {
      "icon": "./ct.ide.icns",
      "copyright": "© Cosmo Myzrail Gorynych 2016-2017"
    },
    "linux": {}
  },
  "scripts": {
    "dist": "build --tasks win-x64,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "dist-win": "build --tasks win-x64 --mirror https://dl.nwjs.io/ .",
    "dist-nowin": "build --tasks linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "start": "run --x86 --mirror https://dl.nwjs.io/ ."
  },
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.14.3"
  }
}
